<Window x:Class="WpfSelectie.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfSelectie"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnTestBool" Content="Test bool" HorizontalAlignment="Left" Margin="10,19,0,0" VerticalAlignment="Top" Width="100" Click="btnTestBool_Click"/>
        <Button x:Name="btnTestNot" Content="Test NOT" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="100" Click="btnTestNot_Click"/>
        <Button x:Name="btnWeekDag" Content="Dag van de week" HorizontalAlignment="Left" Margin="10,111,0,0" VerticalAlignment="Top" Width="100" Click="btnWeekDag_Click"/>
        <Button x:Name="btnLeeftijd" Content="Meerderjarig?" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="100" Click="btnLeeftijd_Click"/>

    </Grid>
</Window>
